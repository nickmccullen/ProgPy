    "N = 1000\n",
    "wins = 0\n",
    "for i in range(N):\n",
    "    side = list(range(1, 6))\n",
    "    A = random.choice(side)\n",
    "    B = random.choice(side)\n",
    "    C = random.choice(side)\n",
    "    if A == B == C:\n",
    "        wins = wins+A\n",
    "    elif A+B+C <= 4:\n",
    "        wins = wins-(A+B+C)\n",
    "print(wins, N)\n",
    "print(1.0 * wins / N)"
   ]
  }
