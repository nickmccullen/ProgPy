   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "dx=0.2\n",
    "xvals = np.arange(-3, 3+dx, dx)\n",
    "\n",
    "yvals = np.exp(-xvals**2)\n",
    "\n",
    "fig, ax = plt.subplots()\n",
    "\n",
    "ax.bar(xvals, yvals, color='cyan', edgecolor=['black']*len(xvals), width=dx)\n",
    "\n",
    "fig.show()"
   ]
  }
