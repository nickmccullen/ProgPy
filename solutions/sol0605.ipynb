   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "xs = np.linspace(0, np.pi, 50)\n",
    "\n",
    "fig, ax = plt.subplots()\n",
    "\n",
    "for n in [1, 3, 5]:\n",
    "    a = 1.0/n\n",
    "    \n",
    "    ys = a*np.sin(n*xs)\n",
    "\n",
    "    ax.plot(xs, ys, label=f'1/{n} sin({n}x)')\n",
    "\n",
    "ax.legend()\n",
    "fig.show()"
   ]
  }
 ],
